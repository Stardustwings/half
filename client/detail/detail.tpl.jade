#detail
  div.title
    span title:
    span #{title}
  div.initiator
    span initiator:
    span #{initiator}
  div.category
    span category:
    span #{category}
  div.status
    span status:
    span #{status}
  div.modifyDate
    span modifyDate:
    span #{modifyDate}
  input(type='hidden', name='voteId', value='#{_id}')

  hr

  img.firstImage(src='#{firstUrl}')
  div.firstDescription
    span firstDescription:
    span #{firstDescription}
  div.numOfFirst
    span numOfFirst:
    span #{numOfFirst}

  hr

  img.secondImage(src='#{secondUrl}')
  div.secondDescription
    span secondDescription:
    span #{secondDescription}
  div.numOfSecond
    span numOfSecond:
    span #{numOfSecond}

  hr

  div.question
    span question:
    span #{question}



