#votelist
  div votelist

  each votelist
    hr

    form.deleteForm
      input(type='hidden', name='voteId', value='#{_id}')
      input(type='hidden', name='firstImageId', value='#{firstImageId}')
      input(type='hidden', name='secondImageId', value='#{secondImageId}')
      button delete

    form.reportForm
      input(type='hidden', name='voteId', value='#{_id}')
      button report

    div.reportNum
      span reportNum:
      span #{reportNum}

    div.edit
      a(href='/editvote/#{_id}') edit

    div.title
      span title:
      a(href='/detail/#{_id}') #{title}
    div.initiator
      span initiator:
      a(href='/profile/#{initiator}') #{initiator}
    div.category
      span category:
      span #{category}
    div.status
      span status:
      span #{status}
    div.modifyDate
      span modifyDate:
      span #{modifyDate}

    img.firstImage(src='#{firstUrl}')
    div.firstDescription
      span firstDescription:
      span #{firstDescription}
    div.numOfFirst
      span numOfFirst:
      span #{numOfFirst}


    img.secondImage(src='#{secondUrl}')
    div.secondDescription
      span secondDescription:
      span #{secondDescription}
    div.numOfSecond
      span numOfSecond:
      span #{numOfSecond}

    div.question
      span question:
      span #{vote.question}
